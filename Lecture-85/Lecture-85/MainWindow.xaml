<Window x:Class="Lecture_85.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lecture_85"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addBotton" Content="Add" HorizontalAlignment="Left" Height="56" Margin="65,297,0,0" VerticalAlignment="Top" Width="144" Click="addBotton_Click"/>
        <Button x:Name="removeBotton" Content="Remove" HorizontalAlignment="Center" Height="56" Margin="0,297,0,0" VerticalAlignment="Top" Width="144" Click="removeBotton_Click"/>
        <Button x:Name="showBotton" Content="Show" HorizontalAlignment="Left" Height="56" Margin="581,297,0,0" VerticalAlignment="Top" Width="144" Click="showBotton_Click"/>
        <TextBox x:Name="nameTextbox" HorizontalAlignment="Left" Height="35" Margin="65,0,0,0" TextWrapping="Wrap" Text="value" VerticalAlignment="Center" Width="525"/>
        <TextBox x:Name="keyTextbox" HorizontalAlignment="Left" Height="36" Margin="65,110,0,0" TextWrapping="Wrap" Text="key" VerticalAlignment="Top" Width="525"/>

    </Grid>
</Window>
